# do not create a queue: section for your test environment
test:
  adapter: postgresql
  encoding: utf8
  database: canvas_test
  host: 127.0.0.1
  username: <%= ENV['PG_USER'] %>
  password: <%= ENV['PG_PASSWORD'] %>
  timeout: 5000

development:
  adapter: postgresql
  encoding: utf8
  database: canvas_development
  host: 127.0.0.1
  username: <%= ENV['PG_USER'] %>
  password: <%= ENV['PG_PASSWORD'] %>
  timeout: 5000

production:
  adapter: postgresql
  encoding: utf8
  database: canvas_production
  host: 127.0.0.1
  username: <%= ENV['PG_USER'] %>
  password: <%= ENV['PG_PASSWORD'] %>
  timeout: 5000
